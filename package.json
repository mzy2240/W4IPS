{
  "name": "PowerWeb",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "vue-cli-service serve"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "echarts": "^4.1.0",
    "echarts-gl": "^1.1.1",
    "fingerprintjs2": "^1.8.1",
    "mapbox-gl": "^0.47.0",
    "mqtt": "^2.18.3",
    "register-service-worker": "^1.0.0",
    "vue": "^2.5.16",
    "vue-drag-zone": "^2.0.0-rc1",
    "vue-echarts": "^3.0.9",
    "vue-grid-layout": "^2.1.13",
    "vue-router": "^3.0.1",
    "vuetify": "^1.1.9",
    "vuex": "^3.0.1",
    "webkul-micron": "^1.1.6"
  },
  "devDependencies": {
    "@mdi/font": "^2.5.94",
    "@vue/cli-plugin-babel": "^3.0.0-rc.10",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.10",
    "@vue/cli-plugin-pwa": "^3.0.0-rc.10",
    "@vue/cli-service": "^3.0.0-rc.10",
    "@vue/eslint-config-prettier": "^3.0.0-rc.10",
    "babel-plugin-component": "^1.1.1",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "vue-cli-plugin-element": "^1.0.0",
    "vue-template-compiler": "^2.5.16"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {
      "no-console": 0,
      "indent": [
        0,
        "tab"
      ],
      "no-unused-vars": 0,
      "prettier/prettier": [
        "warn",
        {
          "singleQuote": true,
          "semi": false,
          "trailingComma": "none",
          "tabWidth": 4
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
